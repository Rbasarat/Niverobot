{
  "ChangeIdDatabaseSettings": {
    "ChangeIdCollectionName": "ChangeId",
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "ChangeId"
  },
  "DefaultNextChangeId": "11175-10283-12549-16025-7812",
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                "outputTemplate": "{Timestamp: HH:mm:ss.fff zzz} [{Level:u3}] {Message:j}{NewLine}{Properties:j}{NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "restrictedToMinimumLevel": "Warning",
                "path": "/Logs/Niverobot.Consumer/info_log.txt",
                "rollingInterval": "Day",
                "fileSizeLimitBytes": 1073741824,
                "rollOnFileSizeLimit": true,
                "flushToDiskInterval": 1
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails"
    ],
    "Properties": {
      "ApplicationName": "Niverobot.Consumer",
      "Environment": "Prod"
    }
  }
}